@model RehabCenters

<h2>Update @Model.Name</h2>


@using (Html.BeginForm("UpdateRehabCentersToDatabase", "RehabCenters", "Post"))
{
    <input asp-for="ID" type="hidden" value="@Model.ID" />

    <label asp-for="Name" class="" control-label">Name</label>
    <input type="text" name="Name" class="form-control" value="@Model.Name" />

    <label asp-for="Address" class="" control-label">Address</label>
    <input type="text" name="Address" class="form-control" value="@Model.Address" />

    <label asp-for="PhoneNumber" class="" control-label">PhoneNumber</label>
    <input type="text" name="PhoneNumber" class="form-control" value="@Model.PhoneNumber" />

    <label asp-for="Website" class="" control-label">Website</label>
    <input type="text" name="Website" class="form-control" value="@Model.Website" />

    <label asp-for="Specialty" class="" control-label">Specialty</label>
    <input type="text" name="Specialty" class="form-control" value="@Model.Specialty" />

    <label asp-for="SpecialtyID" class="" control-label">SpecialtyID</label>
    <input type="text" name="SpecialtyID" class="form-control" value="@Model.SpecialtyID" />

    <input type="submit" value="Update RehabCenter" class="btn btn-primary" />

    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
}

